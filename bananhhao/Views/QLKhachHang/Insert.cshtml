@{
    ViewBag.Title = "Insert";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@using bananhhao.Models;
@model khachhang



@using (Html.BeginForm("Insert", "QLKhachHang", FormMethod.Post))
{

    <div class="form-group">
        <label for="pwd"> Mã Khách hàng:</label>
        <input name="makh" type="text" class="form-control nhapso" value="" minlength="13" maxlength="13" required pattern=".{0}{13,}" title="Nhap ma it hon 13 ki tu" />
    </div>
    <div class="form-group">
        <label for="pwd"> Tên Khách Hàng:</label>
        <input name="tenkh" type="text" class="form-control" value="" required>
    </div>


    <div class="form-group">
        <label for="pwd">Địa Chỉ:</label>
        <input type="text" class="form-control" name="diachi" value="">
    </div>
    <div class="form-group">
        <label for="pwd">Điện Thoại:</label>
        <input type="text" class="form-control nhapso" maxlength="11" name="dt" value="">
    </div>
    <div class="form-group">
        <label for="pwd">CMND:</label>
        <input type="text" class="form-control nhapso" maxlength="9" ng-minlength="9" name="cmnd" value="">
    </div>
    <button type="submit" class="btn btn-default"> Submit</button>

}
