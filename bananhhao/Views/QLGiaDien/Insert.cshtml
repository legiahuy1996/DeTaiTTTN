@{
    ViewBag.Title = "Insert";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@using bananhhao.Models;
@model giadien


@using (Html.BeginForm("Insert", "QLGiaDien", FormMethod.Post))
{

    <div class="form-group">
        <label for="pwd"> Mã bậc:</label>
        <input name="mabac" type="text" class="form-control" value="" required/>
    </div>
    <div class="form-group">
        <label for="pwd"> Tên bậc:</label>
        <input name="tenbac" type="text" class="form-control" value="" required>
    </div>
    

    <div class="form-group">
        <label for="pwd">Từ số KW:</label>
        <input type="text" class="form-control nhapso"  name="tusokw" value="" >
    </div>
    <div class="form-group">
        <label for="pwd">Đến Số KW:</label>
        <input type="text" class="form-control nhapso"  name="densokw" value="">
    </div>
    <div class="form-group">
        <label for="pwd">Đơn giá KW:</label>
        <input type="text" class="form-control nhapso" name="dongia" value="">
    </div>

    <div class="form-group">
        <label for="pwd"> Ngày Áp Dụng:</label>
        <input type="date" class="form-control" name="ngayapdung" value="">
    </div>
    <button type="submit" class="btn btn-default"> Submit</button>

}
